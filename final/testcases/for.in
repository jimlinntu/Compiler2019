Program fortest
Begin
    declare a, b,I as integer;
    %% End with a semicolon
    FOR(I := 1+3+a TO 7+b)
        a := 7+9;
    ENDFOR;
    %% End with no semicolon
    FOR(I := 1 TO 100)
        a := 7+9;
    ENDFOR
    %% DOWNTO
    FOR(I := 7 + 9 - 8 DOWNTO -1)
        a := a + 1;
    ENDFOR
    %% More complicated expression
    FOR(I := 7 + 9 - 8+a DOWNTO -1-b)
        a := a + 1;
    ENDFOR;
End
